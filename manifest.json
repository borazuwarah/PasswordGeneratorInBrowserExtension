{
  "manifest_version": 3,
  "name": "Generador de Contraseñas",
  "version": "1.0",
  "description": "Genera contraseñas aleatorias de la longitud especificada.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}